{
  "name": "@gabortorma/mwm-nuxt-module-template",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c",
  "description": "My new Nuxt module",
  "license": "MIT",
  "repository": {
    "type": "github",
    "url": "git+ssh://git@github.com/GaborTorma/mwm-nuxt-module-template.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && dotenv release-it",
    "lint": "eslint . && pnpm -r lint",
    "lint:fix": "eslint . --fix && pnpm -r lint:fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "postinstall": "nuxt-module-build prepare && nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.2"
  },
  "devDependencies": {
    "@gabortorma/antfu-eslint-config": "^1.5.1",
    "@gabortorma/mwm": "^0.8.1",
    "@nuxt/devtools": "^1.4.2",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.2",
    "@nuxt/test-utils": "^3.14.2",
    "@types/node": "^22.5.5",
    "changelogen": "^0.5.5",
    "dotenv-cli": "^7.4.2",
    "nuxt": "^3.13.2",
    "release-it": "^17.6.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1",
    "vue-tsc": "^2.1.6"
  }
}
